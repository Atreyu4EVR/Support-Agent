{
  "name": "bsc-agent-typescript",
  "version": "1.0.0",
  "description": "TypeScript version of the BSC Support Agent with LangChain.js",
  "main": "dist/agentWithTools.js",
  "type": "commonjs",
  "scripts": {
    "build": "tsc --skipLibCheck",
    "start": "node dist/agentWithTools.js",
    "start:simple": "node dist/simpleAgent.js",
    "start:api": "npx tsx src/apiExample.ts",
    "dev": "npx tsx src/agentWithTools.ts",
    "dev:simple": "npx tsx src/simpleAgent.ts",
    "dev:api": "npx tsx src/apiExample.ts",
    "debug:agent": "npx tsx src/testAgentDebug.ts",
    "clean": "rm -rf dist",
    "test": "echo 'No tests yet'"
  },
  "keywords": [
    "langchain",
    "azure",
    "openai",
    "agent",
    "typescript",
    "byu-idaho"
  ],
  "author": "Ron Vallejo",
  "license": "MIT",
  "dependencies": {
    "@langchain/community": "^0.2.28",
    "@langchain/core": "^0.3.66",
    "@langchain/langgraph": "^0.4.1",
    "@langchain/openai": "^0.2.8",
    "@langchain/pinecone": "^0.1.1",
    "@langchain/textsplitters": "^0.0.3",
    "@pinecone-database/pinecone": "^3.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "langchain": "^0.2.16",
    "uuid": "^10.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.14.0",
    "@types/uuid": "^10.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.16.0",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
