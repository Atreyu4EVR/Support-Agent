{
  "name": "bsc-application",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run docs:build && tsc -b && vite build && npm run copy:docs",
    "copy:docs": "cp -r docs-site/build dist/docs",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "./run.sh",
    "start:concurrent": "concurrently \"npm run start:api\" \"npm run start:web\" --names \"API,WEB\" --prefix-colors \"blue,green\"",
    "start:with-docs": "concurrently \"./run.sh\" \"npm run docs:dev\" --names \"APP,DOCS\" --prefix-colors \"blue,magenta\"",
    "start:all": "concurrently \"npm run start:api\" \"npm run start:web\" \"npm run docs:dev\" --names \"API,WEB,DOCS\" --prefix-colors \"blue,green,magenta\"",
    "start:api": "./start_python_backend.sh",
    "start:api:ts": "cd backend/typescriptVersions && npm run start:api",
    "start:web": "npm run dev",
    "install:all": "npm install && cd backend/typescriptVersions && npm install && cd ../../docs-site && npm install",
    "docs:dev": "cd docs-site && npm start",
    "docs:build": "cd docs-site && npm run build",
    "docs:serve": "cd docs-site && npm run serve",
    "docs:deploy": "cd docs-site && npm run build && az staticwebapp create --name bsc-agent-docs --resource-group bsc-agent-rg --source . --location 'East US 2' --branch main --app-location '/' --output-location 'build'"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.7.0",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.7.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/axios": "^0.9.36",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
