{
  "name": "byui-support-agent",
  "description": "AI-powered support agent for BYU-Idaho students, faculty, and staff",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "author": "BYU-Idaho Information Technology",
  "repository": {
    "type": "git",
    "url": "https://github.com/BYUI-Information-Technology/BSC-Agent"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "npm run start:concurrent",
    "start:concurrent": "concurrently \"npm run start:api\" \"npm run start:web\" --names \"API,WEB\" --prefix-colors \"blue,green\"",
    "start:api": "./start_python_backend.sh",
    "start:web": "npm run dev",
    "install:all": "npm install",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --rmi all",
    "docker:restart": "docker-compose restart",
    "docker:rebuild": "npm run docker:down && npm run docker:build && npm run docker:up",
    "azure:deploy": "./deploy-azure.sh",
    "azure:status": "az containerapp list --resource-group byui-supportagent-p --output table"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "highlight.js": "^11.11.1",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.7.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.20.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
  }
}
